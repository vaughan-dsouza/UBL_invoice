  "type": "library",
  "keywords": [
    "ubl",
    "invoice",
    "ublinvoice",
    "electronic invoice",
    "xml",
    "xml invoice"
  ],
  "homepage": "https://github.com/vaughan-dsouza/UBL_invoice",
  "license": "MIT",
  "authors": [
    {
      "name": "Vaughan D'souza",
      "role": "Developer"
    }
  ],
  "require": {
    "sabre/xml": "^1.5"
  },
  "autoload": {
    "psr-4": {
      "Vaughan\\UBL\\Invoice\\": "src/"
    }
  },
  "require-dev": {
